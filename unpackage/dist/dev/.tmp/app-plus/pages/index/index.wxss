/* 资料列表详情 */
.content .block {
	height: 200px;
	line-height: 200px;
	text-align: center;
	margin: 10px;
	background-color: #F1F1F1;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.block .div-img image {
			width: 9vw;
			height: 9vw;
}
.content .loadingText {
	line-height: 30px;
	text-align: center;
	font-size: 12px;
	color: #999;
}
#preloader_1{
	height: 40px;
	line-height: 40px;
	position:relative;
	text-align: center;
}
#preloader_1 ._span{
	width: 5px;
	height: 5px;
	margin: 0 1px;
	display: inline-block;
	vertical-align: middle;
	background: #9b59b6;
	-webkit-animation: preloader_1 1.5s  infinite ease-in-out;
	        animation: preloader_1 1.5s  infinite ease-in-out;
}
#preloader_1 ._span:nth-child(2){
	-webkit-animation-delay: .2s;
	        animation-delay: .2s;
}
#preloader_1 ._span:nth-child(3){
	-webkit-animation-delay: .4s;
	        animation-delay: .4s;
}
#preloader_1 ._span:nth-child(4){
	-webkit-animation-delay: .6s;
	        animation-delay: .6s;
}
#preloader_1 ._span:nth-child(5){
	-webkit-animation-delay: .8s;
	        animation-delay: .8s;
}
@-webkit-keyframes preloader_1 {
0% {height:5px;background:#9b59b6;}
25% {height:30px;background:#3498db;}
50% {height:5px;background:#9b59b6;}
100% {height:5px;background:#9b59b6;}
}
@keyframes preloader_1 {
0% {height:5px;background:#9b59b6;}
25% {height:30px;background:#3498db;}
50% {height:5px;background:#9b59b6;}
100% {height:5px;background:#9b59b6;}
}
